

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Body</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @posts.each do |post| %>
      <tr>
        <td><%= link_to post.title, post %></td>
        <td><%= truncate(post.body, :ommision => "... Read More", :length => 50) %></td>
        <td>Post By <%= post.user.email %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

